@model IEnumerable<WebForecastMVC.Models.History>

@{
    ViewBag.Title = "History";
}

<h2>History:</h2>
<p>
    @Html.ActionLink("Go To Main Page", "Index", new { controller = "Home" }, htmlAttributes: new { @class = "btn btn-default" })
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ForecastDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TempMin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TempMax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Wind)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Humidity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Summary)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ForecastDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TempMin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TempMax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wind)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Humidity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Summary)
        </td>        
    </tr>
}

</table>
